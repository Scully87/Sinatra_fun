<div style='border: 5px dashed black'>
  <% if @visitor %>  
    Hello, <%= @visitor %>!
  <% else %>
    <form action=/hello>
      My name is <%= @name %>.
      What's your name?
      <input type=text name=name>
      <input type=submit>
    </form>
  <% end %>
  <img src='/images/denise.png'>
</div>
